        "func": "const { Op } = global.get('sequelize');\nconst { FlowDataModel, DeviceDataModel, DeviceStateDataModel } = global.get(\"db\");\nconst flowUid = node.path.split('/').find(_ => true);\n\nconst devices = await DeviceDataModel.findAll({\n    attributes: ['id'],\n    include: [{\n        model: FlowDataModel,\n        as: 'flow',\n        attributes: [],\n        where: {\n            uid: flowUid\n        }\n    }]\n});\n\n\ndevices.forEach(async (d) => {\n    const state = flow.get(`state${d.id}`);\n\n    await DeviceStateDataModel.create({\n        deviceId: d.id,\n        state: JSON.stringify(state)\n    });\n});\n\n\nreturn msg;",
